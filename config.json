{
  "endpoint" : "http://192.168.1.1:79/rci",
  "interval_sec" : 10,
  "metrics" : [
    {
      "command": "processes",
      "root" : "$.process.*",
      "tags" : {
        "name": "~",
        "process_name" : "$.name",
        "pid": "$.pid"
      },
      "values" : {
        "cpu": "$.statistics.cpu.cur",
        "memory": "$.vm-size"
      }
    },
    {
      "command": "ip hotspot",
      "root" : "$.host[*]",
      "tags" : {
        "hostname" : "$.hostname",
        "name" : "$.name"
      },
      "values" : {
        "active": "$.active"
      }
    },
    {
      "command": "ip nat",
      "root" : "$.[*]",
      "tags" : {
        "src" : "$.src",
        "dst" : "$.dst"
      },
      "values" : {
        "bytes-in": "$.bytes",
        "bytes-out": "$.bytes-out"
      }
    }
  ]
}